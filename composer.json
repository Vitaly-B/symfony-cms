{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.0.8",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/user-bundle": "^2.0",
        "gedmo/doctrine-extensions": "^2.4",
        "incenteev/composer-parameter-handler": "^2.0",
        "jms/i18n-routing-bundle": "^2.0",
        "knplabs/doctrine-behaviors": "^1.4",
        "liip/imagine-bundle": "^1.8",
        "phpunit/phpunit": "^6.3",
        "redcode/tree-bundle": "dev-master",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^3.0.2",
        "sonata-project/admin-bundle": "^3.22",
        "sonata-project/doctrine-orm-admin-bundle": "^3.1",
        "sonata-project/formatter-bundle": "^3.2",
        "sonata-project/intl-bundle": "^2.3",
        "sonata-project/media-bundle": "^3.8",
        "sonata-project/translation-bundle": "^2.1",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/assetic-bundle": "^2.8",
        "symfony/browser-kit": "^3.3",
        "symfony/console": "^3.3",
        "symfony/framework-bundle": "^3.3",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.3.10",
        "symfony/yaml": "^3.3",
        "twig/twig": "^1.0 || ^2.0",
        "white-october/pagerfanta-bundle": "^1.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^2.4",
        "sensio/generator-bundle": "^3.0",
        "symfony/dotenv": "^3.3",
        "symfony/flex": "^1.0",
        "symfony/phpunit-bridge": "^3.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "^3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BXHNATF0PH6Y5KBQF6J3TX87",
            "allow-contrib": false
        }
    }
}
